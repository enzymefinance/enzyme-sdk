{
  "name": "@enzymefinance/sdk",
  "version": "0.0.9",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/enzymefinance/enzyme-sdk.git",
    "directory": "packages/sdk"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm run clean && pnpm run build:cjs && pnpm run build:esm+types",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --verbatimModuleSyntax false --outDir ./dist/cjs && echo > ./dist/cjs/package.json '{\"type\":\"commonjs\"}'",
    "build:esm+types": "tsc --project tsconfig.build.json --module es2015 --outDir ./dist/esm --declaration --declarationMap --declarationDir ./dist/types && echo > ./dist/esm/package.json '{\"type\":\"module\"}'",
    "release": "pnpm build && ../../scripts/prerelease.sh ./package.json",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist/**",
    "!dist/**/*.tsbuildinfo",
    "src/**/*.ts",
    "!src/**/*.test.ts"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "typings": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@enzymefinance/abis": "^0.0.8",
    "abitype": "^0.7.1",
    "decimal.js": "^10.4.3",
    "viem": "^0.3.1"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@vitest/coverage-c8": "^0.29.2",
    "@vitest/ui": "^0.29.2",
    "execa": "^7.1.1",
    "typescript": "^5.0.3",
    "undici": "^5.21.2",
    "vite": "^4.2.1",
    "vite-tsconfig-paths": "^4.0.8",
    "vitest": "^0.29.8"
  }
}
