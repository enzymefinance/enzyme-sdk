{
  "name": "root",
  "private": true,
  "packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228",
  "scripts": {
    "codegen": "pnpm run --recursive codegen",
    "build": "tsc --build tsconfig.build.json && pnpm run --recursive --filter \"./packages/*\" build",
    "release": "pnpm build && changeset publish",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --build tsconfig.json",
    "documentation": "pnpm run --filter docs dev",
    "format": "biome format --write . && biome check --write --unsafe .",
    "lint": "biome check ."
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-export-namespace-from": "^7.25.9",
    "@babel/plugin-transform-modules-commonjs": "^7.25.9",
    "@biomejs/biome": "1.8.3",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.10",
    "@effect/build-utils": "^0.7.8",
    "@types/node": "^22.9.1",
    "@types/web": "^0.0.181",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.3",
    "vitest": "^2.1.5"
  }
}
