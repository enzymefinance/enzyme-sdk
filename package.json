{
  "name": "root",
  "private": true,
  "packageManager": "pnpm@8.12.1",
  "scripts": {
    "codegen": "pnpm run --recursive codegen",
    "build": "tsc --build tsconfig.build.json && pnpm run --recursive --filter \"./packages/*\" build",
    "release": "pnpm build && changeset publish",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --build tsconfig.json",
    "documentation": "pnpm run --filter @enzymefinance/docs dev",
    "format": "biome format --write . && biome check --apply-unsafe .",
    "lint": "biome check ."
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/plugin-transform-export-namespace-from": "^7.23.4",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@biomejs/biome": "^1.4.1",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@effect/build-utils": "^0.7.1",
    "@types/node": "^20.10.8",
    "@types/web": "^0.0.128",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-tsconfig-paths": "^4.2.3",
    "vitest": "^1.1.3"
  },
  "pnpm": {
    "patchedDependencies": {
      "@changesets/assemble-release-plan@6.0.0": "patches/@changesets__assemble-release-plan@6.0.0.patch"
    }
  }
}
