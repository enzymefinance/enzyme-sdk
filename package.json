{
  "name": "root",
  "private": true,
  "packageManager": "pnpm@8.6.1",
  "scripts": {
    "codegen": "pnpm run --recursive codegen",
    "build": "tsc --build && pnpm run --recursive --filter \"./packages/*\" build",
    "release": "changeset version && pnpm run build && changeset publish",
    "test": "pnpm run --recursive --filter \"./packages/*\" test --run",
    "test:coverage": "pnpm run --recursive --filter \"./packages/*\" test:coverage",
    "clean": "pnpm run --recursive --parallel clean",
    "typecheck": "pnpm run --recursive --parallel typecheck",
    "format": "biome format --write . && biome check --apply-unsafe .",
    "lint": "biome check ."
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.11",
    "@babel/plugin-transform-export-namespace-from": "^7.22.11",
    "@babel/plugin-transform-modules-commonjs": "^7.22.11",
    "@biomejs/biome": "^1.2.2",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@effect/build-utils": "^0.1.4",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "bun": "^1.0.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.2.2"
  },
  "pnpm": {
    "overrides": {
      "@enzymefinance/api": "workspace:*",
      "@enzymefinance/abis": "workspace:*",
      "@enzymefinance/sdk": "workspace:*"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "prop-types"
      ]
    }
  }
}
