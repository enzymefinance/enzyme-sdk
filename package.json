{
  "name": "root",
  "private": true,
  "packageManager": "pnpm@8.10.0",
  "scripts": {
    "codegen": "pnpm run --recursive codegen",
    "build": "tsc --build tsconfig.build.json && pnpm run --recursive --filter \"./packages/*\" build",
    "release": "changeset version && pnpm run build && changeset publish",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "pnpm run --recursive --parallel typecheck",
    "format": "biome format --write . && biome check --apply-unsafe .",
    "lint": "biome check ."
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/plugin-transform-export-namespace-from": "^7.22.11",
    "@babel/plugin-transform-modules-commonjs": "^7.23.0",
    "@biomejs/biome": "^1.3.3",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@effect/build-utils": "^0.3.0",
    "@types/node": "^20.9.0",
    "@types/web": "^0.0.119",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "pnpm": {
    "overrides": {
      "@enzymefinance/api": "workspace:*",
      "@enzymefinance/abis": "workspace:*",
      "@enzymefinance/sdk": "workspace:*"
    }
  }
}
